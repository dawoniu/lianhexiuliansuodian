<view><view class="map"><map style="width:100%;height:calc(100vh - 290px);" id="myMap" show-compass="true" scale="16" show-location="{{true}}" latitude="{{latitude}}" longitude="{{longitude}}"><cover-view data-event-opts="{{[['tap',[['moveToLocation',['$event']]]]]}}" class="location" bindtap="__e"><cover-image src="/static/images/dangqian.png" mode="widthFix"></cover-image></cover-view></map></view><view class="main"><view class="city">{{''+dangqian+''}}</view><view class="content"><view class="storename">{{''+data.store.storename+''}}</view><view class="address"><block wx:if="{{address_count>0}}"><scroll-view scroll-x="true"><block wx:for="{{data.adr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><block wx:if="{{item.dft}}"><view class="item check">{{''+item.linkman+''}}</view></block><block wx:else><view data-event-opts="{{[['tap',[['setDefault',['$0',index],[[['data.adr','',index,'id']]]]]]]}}" class="item" bindtap="__e">{{''+item.linkman+''}}</view></block></block></block></scroll-view></block><block wx:else><view data-event-opts="{{[['tap',[['addAdr',['$event']]]]]}}" class="item add" bindtap="__e">请先添加地址</view></block><view data-event-opts="{{[['tap',[['addAdr',['$event']]]]]}}" class="item add" bindtap="__e">+</view></view><block wx:if="{{address_count>0}}"><view class="linkman"><view class="left"><image src="../../static/images/address_people.png" mode="widthFix"></image></view><view class="right">{{''+linkman+''}}</view></view></block><block wx:if="{{address_count>0}}"><view class="position_address"><view class="left"><image src="../../static/images/address_position.png" mode="widthFix"></image></view><view class="right">{{''+data.store.position_address+''}}</view></view></block><view class="sub"><button type="default" data-event-opts="{{[['tap',[['order',['$event']]]]]}}" bindtap="__e">立即下单</button></view></view></view></view>